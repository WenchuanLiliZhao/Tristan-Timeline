{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "typescript-config/consistent-casing": "off",
    "axe/aria": [
      "default",
      {
        "aria-roles": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scrollbar-width: thin",
          "scrollbar-color",
          "scrollbar-width: none",
          "user-select"
        ]
      }
    ]
  }
}